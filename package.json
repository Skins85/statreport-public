    "body-parser": "^1.19.0",
    "chart.js": "^2.9.4",
    "concurrently": "^6.0.0",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "moment-timezone": "^0.5.33",
    "mysql": "^2.18.1",
    "node-env-run": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^2.1.0",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.3",
    "react-dom": "^17.0.1",
    "react-moment": "^1.1.1",
