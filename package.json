{
  "name": "statreport-webpack",
  "version": "1.0.0",
  "description": "StatReport site bundled with Webpack",
  "main": "index.js",
  "scripts": {
    "start-server": "node server/index.js",
    "run-server": "nodemon server/index.js",
    "start": "webpack serve --open --config webpack.dev.js --hot",
    "build": "webpack --config webpack.prod.js",
    "server": "nodemon babel-node server/index.js",
    "dev": "run-p start run-server",
    "bundle-report": "webpack-bundle-analyzer --port 4200 dist/stats.json"
  },
  "proxy": "http://localhost:3001",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
